<Window x:Class="WpfApp14.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp14"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid Name="Grid1">
        <TextBlock Margin="10" Text="{Binding Path=Name}"/>
        <TextBlock Margin="20" Text="{Binding Path=Surname}"/>
        <TextBlock Margin="30" Text="{Binding Path=Company.CompanyName}"/>
        <Button Content="btn" Height="20" Width="60" Margin="353,234,104,65" Click="Button_Click" />
    </Grid>
</Window>
